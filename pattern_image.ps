/pixel{ %str w h

    5 dict begin
    /.j exch def
    /.i exch def
    /.h exch def
    /.w exch def
    /.str exch def

    .w .h mul .str length ne{
        (/pixel: Size error) /rangeerror signalerror
        }if

    .str .w .j mul .i add get
     end
}bind def


/averageKernel.Gray{ % Assumes str .w .h x y size from /averageFilterGray
    2 dict begin
    /d .size 2 div cvi def

    x d ge
    y d ge
    and
    x .w d sub lt
    and
    y .h d sub lt
    and
    {
        /val 0 def
        d neg 1 d{
            /j exch def
            d neg 1 d{
                /i exch def
                /val val   .str .w .h x i add y j add pixel add store
            }for
        }for
        val .size .size mul div cvi
    }{
        str .w .h x y pixel cvi
    }ifelse
    end
}def

/averageFilter.Gray{ % w h str size
    (/averageFilterGray) =
    1 dict begin
    /.size exch def
    /.h exch def
    /.w exch def
    /.str exch def

    /newstr .str length string def

    0 1 .h 1 sub{
        /y exch def
        
         0 1 .w 1 sub{
             /x exch def
             newstr y .w mul x add
             averageKernel.Gray
             put
         }for
    }for
    
    newstr
    end
}bind def



