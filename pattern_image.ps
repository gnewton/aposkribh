/noiseFillColorThisColor2{
    (noiseFillColorThisColor2: START) =
    10 dict begin
    /d# exch def

    /.color d# /Color get def

    d# /Filter known{
        /.filter d# /Filter get def
    }{
        /.filter false def
    }ifelse

    .filter{
        d# /FilterProc known{
            /.filterProc d# /FilterProc get def
        }{
            (noiseFillColorThisColor2: Missing filter proc) /stackunderflow signalerror
        }ifelse
    }if

    /.x0 d# /X0 get def
    /.y0 d# /Y0 get def
    /.x1 d# /X1 get def
    /.y1 d# /Y1 get def
    /.scale d# /Scale get def
    /.deltaPlusMinus d# /DeltaPlusMinus get def

     /width x1 x0 sub .scale mul cvi def
     /height y1 y0 sub .scale mul cvi def
    

    .color{
	/.blue d# /Blue get def
	/.green d# /Green get def
	/.red d# /Red get def
    }{
	/.gray d# /Gray get def
    }ifelse


    %/stringLength height width add 2 div dup mul 3 mul 3 add cvi def
    /stringLength height width mul def
    %/stringLength width height mul 3 mul cvi def
    /str stringLength  string def

    gsave
    .x0 .y0 translate

    % Image parameters:
    %width .scale mul cvi
    %height .scale mul cvi
    width 
    height
    8                       % bits/sample
    [.scale 0 0 .scale 0 0] % matrix
    {
	.color{
	    0 3 stringLength 1 sub
	}{ 
	    0 1 stringLength 1 sub % grayscale
	}ifelse
	{
	    /n exch def
	    .color{
		str n        .red   rand .deltaPlusMinus mod plusMinus  0 255 range  put
		str n 1 add  .green rand .deltaPlusMinus mod plusMinus  0 255 range  put
		str n 2 add  .blue  rand .deltaPlusMinus mod plusMinus  0 255 range  put
	    }{
                % grayscale
		str n        .gray   rand .deltaPlusMinus mod plusMinus  0 255 range  put 
	    }ifelse
	} for
        (NNN) =
        .filter{
            .filterProc
        }{
            str
        }ifelse
    }                      % datasrc
    .color{
        false
        3
        colorimage % Color Image
    }{
        % grayscale
        image
    }ifelse

    grestore
    end

} bind def


/colorPixel{
    [
    str i get
    str i 1 add get
    str i 2 add get
    ]
}bind def


/averageFilter.Color{
    (/averageFilter.Color START) =

    /newstr str length string def

    0 1 str length 1 sub 3 sub{
        /i exch def
        newstr i         str i       get  2 div  cvi 0 255 range put
        newstr i 1 add   str i 1 add get  2 div  cvi 0 255 range put
        newstr i 2 add   str i 2 add get  2 div  cvi 0 255 range put
        %str i get =
    }for
    
     0 1 height 1 sub{
         /y exch def
        
         0 3 width 1 sub{
             /x exch def
             /i y width mul  x add def
             colorPixel pop
             newstr i         str i       get   2 div cvi put
             newstr i 1 add   str i 1 add get   2 div cvi put
             newstr i 2 add   str i 2 add get   2 div cvi put
         }for
     }for
    
     newstr
    % end
}bind def



