-50 150 1200  -100 150 1200
{
    2 dict begin
    /y exch def
    /x exch def

    newpath
    
    x y moveto
    Poly# begin

    gsave
    160 6 x  y drawpoly
    gsave
    .7 .setfillconstantalpha
    .4 0 0 setrgbcolor
    fill
    grestore

    gsave
    .5 .setstrokeconstantalpha
    1 setgray
    stroke
    grestore
    grestore


    grestore
    end % Poly#
    end
    
}forxy

